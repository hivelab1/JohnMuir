      copyright: "\"Creative Commons\uFFFDAttribution 3.0 Unported\uFFFDlicense, author:
        Bobak Ha'Eri\""
      location: '"175 Yaney Ave, Sonora, CA 95370"; Lat: 37.984134 Lon: -120.386118'
      resourceURI: 
      resourceImage: {fileID: 2800000, guid: 54bbeb8d254260d46bb16c7cbe76ed1e, type: 3}
